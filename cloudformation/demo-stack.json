{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "Custom Provider CloudFormation",
  "Resources": {
    "Custom": {
      "Type": "Custom::Custom",
      "Properties": {
        "Value":  "hello World!",
        "ServiceToken": { "Fn::Join": [ ":", [ "arn:aws:lambda", { "Ref": "AWS::Region" }, { "Ref": "AWS::AccountId" }, "function:cfn-custom-provider" ]]}
      }
    }
  },
  "Outputs": {
     "Value": { 
       "Description": "The value returned by the Custom::Custom resource",
       "Value": {"Fn::GetAtt": [ "Custom", "Value" ]}
     }
  }
}
